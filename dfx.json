{
  "canisters": {
    "CrystalBank": {
      "main": "backend/PlayerRegistration.mo",
      "type": "motoko"
    },
    "frontend": {
      "dependencies": [
        "CrystalBank"
      ],
      "source": [
        "frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    }
  },
  "defaults": {
    "build": {
      "output": "build",
      "packtool": "mops sources"
    },
    "network": "local"
  },
  "version": 1
}